<html>
<head>
    <title>Show list</title>
    <style>
        #contents{
            background-color:lavender;
        }
    </style>
</head>
<body>
    <ol>
        <%shows.forEach(function(show){ %>
        <div id="contents"><li>
            <h2><a href="">제목 : <%=show.show.title%></a></h2>
            postType : <%=show.postType%><br>
            createAt : <%=show.createAt%><br>
            _writer : <%=show._writer%><br>
            content : <%=show.content%><br>
            hashTags : <%=show.hashTags%><br>
            likes : <%=show.likes%><br>
                <% show.resources.forEach(function(resource){%>
                    resources : <%=resource%><br>
                    pic : <img src="<%=resource.originalPath%>" width="200" height="200"/><br>
                <%})%><br>
            show.type : <%=show.show.type%><br>
            show.startDate : <%=show.show.startDate%><br>
            show.endDate : <%=show.show.endDate%><br>
            show.startTime : <%=show.show.startTime%><br>
            show.endTime : <%=show.show.endTime%><br>
            show.fee : <%=show.show.fee%><br>
            show.location : <img src="http://maps.googleapis.com/maps/api/staticmap?center=<%=show.show.location.point.coordinates[0]%>,<%=show.show.location.point.coordinates[1]%>&zoom=9&scale=true&size=400x200&maptype=roadmap&format=png&visual_refresh=true&markers=size:mid|color:red|label:<%=show.show.location.address%>|<%=show.show.location.point.coordinates[0]%>,<%=show.show.location.point.coordinates[1]%>"/>
        </li></div>
        <% });  %>
    </ol>
</body>
</html>